{
  "name": "json-schema-yup-transformer",
  "author": "Ritchie Anesco <info@ritchieanesco.com>",
  "version": "1.0.0",
  "description": "Transforms a draft 7 specification JSON Schema to a Yup Schema",
  "license": "MIT",
  "main": "src/index.ts",
  "scripts": {
    "build": "parcel build src/index.ts --out-dir dist",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --runInBand",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "is-relative-url": "3.0.0",
    "lodash": "4.17.15",
    "yup": "0.28.3"
  },
  "devDependencies": {
    "@types/is-relative-url": "^3.0.0",
    "@types/jest": "^25.1.4",
    "@types/json-schema": "^7.0.4",
    "@types/lodash": "^4.14.149",
    "@types/yup": "^0.26.33",
    "coveralls": "^3.0.11",
    "jest": "^25.1.0",
    "parcel-bundler": "^1.12.4",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "keywords": [],
  "jest": {
    "testEnvironment": "node"
  }
}
